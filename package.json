{
  "name": "@cdssnc/pre-flight",
  "version": "0.0.1",
  "description": "CDS pre-flight checks",
  "main": "src/index.js",
  "scripts": {
    "dev": "node serve",
    "lint": "eslint src/*.js",
    "test": "jest",
    "start": "NODE_ENV=production node index"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@cdssnc/logdriver": "^0.0.2",
    "@octokit/app": "^2.2.2",
    "@octokit/rest": "^16.22.0",
    "dotenv-safe": "^6.1.0",
    "esm": "^3.2.22",
    "list-files-in-dir": "^0.1.5",
    "node-fetch": "^2.3.0",
    "nodejs-base64": "^1.0.3",
    "serverless": "^1.40.0",
    "serverless-google-cloudfunctions": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-eslint": "^10.0.1",
    "body-parser": "^1.18.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "nock": "^10.0.6",
    "prettier": "^1.16.4",
    "supertest": "^4.0.2"
  }
}
